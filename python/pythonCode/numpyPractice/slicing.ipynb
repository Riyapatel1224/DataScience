   ],
   "source": [
    "import numpy as np\n",
    "\n",
    "var = np.array([1,2,3,4,5,6,7])\n",
    "\n",
    "print(\"4 to 6 : \",var[3:6])\n",
    "print(\"odd: \",var[::2])\n",
    "print(\"even: \",var[1:(len(var)):2])"
   ]
  }
